{
  "name": "twitch-app-6",
  "version": "1.0.0",
  "description": "twitch-app-6",
  "keywords": [
    "twitch",
    "electron",
    "typescript"
  ],
  "author": "rr1980",
  "license": "ISC",
  "main": "./dist/main.js",
  "scripts": {
    "build": "webpack --config webpack.main.js && webpack --config webpack.renderer.js",
    "build:prod": "webpack --config webpack.main.js --mode=production && webpack --config webpack.renderer.js --mode=production",
    "start": "npm run build && electron ./dist/main.js",
    "dist": "npm run build:prod && electron-builder"
  },
  "build": {
    "appId": "twitch.app.6",
    "asar": true,
    "directories": {
      "buildResources": "./dist",
      "output": "build"
    },
    "files": [
      "dist/**"
    ],
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "dist//icons/icon.png"
    }
  },
  "dependencies": {
    "electron-aspectratio": "^1.0.1",
    "electron-localshortcut": "^3.2.1"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "electron": "^7.1.2",
    "electron-builder": "^21.2.0",
    "minify": "^4.1.3",
    "rxjs": "^6.5.3",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
